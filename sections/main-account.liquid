{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  .btn-diagnostic {
  background: #D0C4E2;
    border-radius: 30px;
    color: white;
    font-weight: bold;
    padding: 0.8rem 2rem;
    
    min-width: 100%;
    box-sizing: border-box;
  }
    .container-options > .option a {
    z-index:2;
          display: block;
      position:relative;
  }
  .container-options, .container-content {
    border-radius:20px;
    border:2px solid #FBDAE2;
    position:Relative;
    overflow:hidden;
  }
  .container-content h3 {
    font-weight:bold;
    margin-bottom:20px;
  }
    .container-content h2 {
    font-weight:bold;
      text-align:center;
    margin-bottom:10px;
  }
   .container-options > div.active {
     background:#FBDAE2;
     color:#575756;
   } 
   .container-options > div {
     padding:10px 14px;
     padding-bottom:20px;
     border-bottom:1px solid #FBDAE2;
   }
  .container-options > div:last-child {
    border:none;
  }
  thead, tbody tr {
    border-bottom:1px solid #FBDAE2;
  }
 tbody tr:last-child {
    border-bottom:0px;
  }
  table {
    width: 100%;
  }
  thead th, tbody th,  tbody {
    text-align:center;
  }
    .row-ingredientes {
      margin-top:20px;
    }
  .row-ingredientes img {
    width:64px;
    margin:30px auto;
   
  }
   .row-ingredientes .container-color {
      border:2px solid #FBDAE2;
     border-radius:30px;
     padding:15px;
   }
 .diagnostics  
  {
    font-size:12px;
  }
  
 .diagnostics  p
  {
    font-size:14px;
  }
.option.active svg {
  display:block;
}
  .option svg {
  display:none;
}
  .diagnostics h4 {
    text-align:center;
    font-weight:600;
    margin-bottom:20px;
  }
{%- endstyle -%}

